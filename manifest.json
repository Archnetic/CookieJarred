{
    "description": "Automagically tells websites to denies permissions to store cookies",
    "manifest_version": 2,
    "homepage_url": "https://github.com/ChasemLose/CookieJarred",
    "name": "Cookie Jarred",
    "permissions": ["tabs", "webNavigation", "*://*.gov/*",
         "*://*.com/*" , "*://*.net/*", "*://*.org/*", "*://*.edu/*"],
    "background": {
        "scripts": ["jarred.js"]
    },
    "version": "1.0",
    "applications": {
        "gecko": {
            "strict_min_version": "55.0a2"
        }
    },
    "browser_action": {
        "default_icon": "cookies-from-Freepik-on-flaticon.com.png",
        "default_title": "Cookie Jarred",
        "default_popup": "home.html"
      }
}